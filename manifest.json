{
    "manifest_version": 3,
    "name": "Smule Utility Extension",
    "description": "Provides extra tools on Smule to use",
    "version": "1.0",
    "permissions": [
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["https://www.smule.com/*"],
            "js": ["src/scripts/songdownload.js"],
            "css": [],
            "all_frames" : true
        }
    ]
}